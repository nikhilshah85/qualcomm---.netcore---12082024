@{
}

<table>
    @foreach (var item in ViewBag.Comments)
    {
        <tr>
            <td> item.postId </td>
            <td> item.Id </td>
            <td> item.name </td>
            <td> item.email </td>
            <td> item.body </td>

        </tr>
    }
</table>